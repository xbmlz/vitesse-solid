{
  "name": "vitesse-solid",
  "type": "module",
  "packageManager": "pnpm@7.17.1",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "postinstall": "node add-ts-nocheck.cjs",
    "dev": "solid-start dev --host --port 3333",
    "clean": "rimraf .solid .vercel .netlify netlify dist",
    "build": "rimraf dist && solid-start build",
    "build:static": "rimraf dist && cross-env ADAPTER=static solid-start build",
    "build:netlify": "rimraf netlify .netlify && cross-env ADAPTER=netlify solid-start build",
    "build:vercel": "rimraf .vercel && cross-env ADAPTER=vercel solid-start build",
    "build:aws": "rimraf dist && cross-env ADAPTER=aws solid-start build",
    "test": "vitest run",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "lint": "eslint . --ext .ts,.tsx,.md,.mdx",
    "lint:fix": "eslint . --fix --ext .ts,.tsx,.md,.mdx",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "dependencies": {
    "@hope-ui/core": "1.0.0-next.11",
    "@solid-primitives/i18n": "^1.1.2",
    "@solid-primitives/media": "^2.0.4",
    "@solid-primitives/storage": "^1.3.3",
    "@stitches/core": "^1.2.8",
    "@tauri-apps/api": "^1.2.0",
    "clsx": "^1.2.1",
    "solid-js": "^1.6.2",
    "solid-start": "^0.2.5",
    "solid-toast": "^0.4.0"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.11",
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.5.0",
    "@solidjs/testing-library": "^0.5.1",
    "@tauri-apps/cli": "^1.2.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@xbmlz/eslint-config-prettier": "^0.1.16",
    "@xbmlz/eslint-config-solid": "^0.1.16",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^12.0.2",
    "eslint": "^8.29.0",
    "jsdom": "^20.0.2",
    "pnpm": "^7.17.1",
    "rehype-pretty-code": "^0.5.1",
    "remark-gfm": "^3.0.1",
    "shiki": "^0.11.1",
    "solid-start-aws": "^0.2.7",
    "solid-start-netlify": "^0.2.7",
    "solid-start-node": "^0.2.7",
    "solid-start-static": "^0.2.7",
    "solid-start-vercel": "^0.2.7",
    "typescript": "^4.9.3",
    "undici": "^5.12.0",
    "unocss": "^0.47.5",
    "unplugin-auto-import": "^0.12.0",
    "vite": "^3.2.4",
    "vite-plugin-pwa": "^0.13.3",
    "vite-plugin-solid-markdown": "^0.0.4",
    "vitest": "^0.25.2"
  }
}
